#!/usr/bin/env bash
# This script generates an RSA key pair with a private key named 'school' and a passphrase 'betty'

# Generate the RSA key pair
ssh-keygen -t rsa -b 4096 -C "school" -N "betty" -f school

# Display success message
echo "RSA key pair generated successfully."
echo "Private key: school"
echo "Public key: school.pub"
